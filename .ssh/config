AddKeysToAgent yes

Host github.com
  User git
  IdentityFile ~/.ssh/id_rsa_github

Host gitlab.isae-supaero.fr
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa_gitlab.isae-supaero

Host gitlab.com
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa_gitlab

Host gitlab.freedesktop.org
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa_gitlab.freedesktop

