AddKeysToAgent yes

Host github.com
  User git
  IdentityFile ~/.ssh/keys/github

Host gitlab.isae-supaero.fr
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/keys/gitlab.isae-supaero

Host gitlab.com
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/keys/gitlab

Host gitlab.freedesktop.org
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/keys/gitlab.freedesktop

